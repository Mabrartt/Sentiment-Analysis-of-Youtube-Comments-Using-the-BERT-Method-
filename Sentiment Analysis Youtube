    {
      "cell_type": "markdown",
      "source": [
        "Text Processing"
      ],
      "metadata": {
        "id": "3hRjsXAFpZv2"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# Fungsi untuk menghapus emoji menggunakan pattern Unicode\n",
        "def strip_emoji(text):\n",
        "    emoji_pattern = re.compile(\n",
        "        \"[\"\n",
        "        u\"\\U0001F600-\\U0001F64F\"  # emoticon\n",
        "        u\"\\U0001F300-\\U0001F5FF\"  # simbol & piktogram\n",
        "        u\"\\U0001F680-\\U0001F6FF\"  # transport & simbol\n",
        "        u\"\\U0001F1E0-\\U0001F1FF\"  # bendera (negara)\n",
        "        u\"\\U00002702-\\U000027B0\"\n",
        "        u\"\\U000024C2-\\U0001F251\"\n",
        "        \"]+\", flags=re.UNICODE)\n",
        "    return emoji_pattern.sub(r'', text)\n",
        "\n",
        "# Fungsi untuk menghapus punctuations, link, mention, dan karakter baru (\\r\\n)\n",
        "def strip_all_entities(text):\n",
        "    text = text.replace('\\r', '').replace('\\n', ' ').lower()  # hapus \\n dan \\r dan ubah menjadi huruf kecil\n",
        "    text = re.sub(r\"(?:\\@|https?\\://)\\S+\", \"\", text)  # hapus link dan mention\n",
        "    text = re.sub(r'[^\\x00-\\x7f]',r'', text)  # hapus karakter non-utf8/ascii\n",
        "    banned_list = string.punctuation + 'Ã' + '±' + 'ã' + '¼' + 'â' + '»' + '§'\n",
        "    table = str.maketrans('', '', banned_list)\n",
        "    text = text.translate(table)\n",
        "    return text\n",
        "\n",
        "# Fungsi untuk membersihkan hashtag\n",
        "def clean_hashtags(tweet):\n",
        "    new_tweet = \" \".join(word.strip() for word in re.split('#(?!(?:hashtag)\\b)[\\w-]+(?=(?:\\s+#[\\w-]+)*\\s*$)', tweet))  # hapus hashtag terakhir\n",
        "    new_tweet2 = \" \".join(word.strip() for word in re.split('#|_', new_tweet))  # hapus simbol hashtag dari kata-kata di tengah kalimat\n",
        "    return new_tweet2\n",
        "\n",
        "# Fungsi untuk memfilter karakter khusus seperti & dan $\n",
        "def filter_chars(a):\n",
        "    sent = []\n",
        "    for word in a.split(' '):\n",
        "        if ('$' in word) | ('&' in word):\n",
        "            sent.append('')\n",
        "        else:\n",
        "            sent.append(word)\n",
        "    return ' '.join(sent)\n",
        "\n",
        "# Fungsi untuk menghapus spasi ganda\n",
        "def remove_mult_spaces(text):\n",
        "    return re.sub(\"\\s\\s+\", \" \", text)"
      ],
      "metadata": {
        "id": "SkTmFahYnFbL"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Proses teks untuk df_train\n",
        "texts_new = []\n",
        "for t in df_train.text:\n",
        "    clean_text = strip_emoji(t)\n",
        "    clean_text = strip_all_entities(clean_text)\n",
        "    clean_text = clean_hashtags(clean_text)\n",
        "    clean_text = filter_chars(clean_text)\n",
        "    clean_text = remove_mult_spaces(clean_text)\n",
        "    texts_new.append(clean_text)"
      ],
      "metadata": {
        "id": "KSCoQnFAnKvH"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Proses teks untuk df_test\n",
        "texts_new_test = []\n",
        "for t in df_test.text:\n",
        "    clean_text = strip_emoji(t)\n",
        "    clean_text = strip_all_entities(clean_text)\n",
        "    clean_text = clean_hashtags(clean_text)\n",
        "    clean_text = filter_chars(clean_text)\n",
        "    clean_text = remove_mult_spaces(clean_text)\n",
        "    texts_new_test.append(clean_text)"
      ],
      "metadata": {
        "id": "fGX6oOpsoQo8"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Tambahkan teks yang sudah dibersihkan ke dataframe\n",
        "df_train['clean_text'] = texts_new\n",
        "df_test['clean_text'] = texts_new_test"
      ],
      "metadata": {
