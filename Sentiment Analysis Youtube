        "# Make a prediction on a new text\n",
        "new_text = \"yeah designwise when did apple get so muted with their hardware colors but more importantlythey used to make the new product look different as a sort of social proof that you got the latest and greatest which drives sales between the airpods all looking the same and now the pencilshow lazyscared is this design dept since jony left dont even get me going on how the os still feels like its from 2013 steve would be aghast.\"\n",
        "\n",
        "# Tokenize the text\n",
        "input_ids, attention_masks, token_type_ids = tokenize([new_text], MAX_LEN)\n",
        "\n",
        "# Make a prediction\n",
        "result = model.predict([input_ids, attention_masks])\n",
        "\n",
        "# Get the predicted class\n",
        "predicted_class = np.argmax(result)\n",
        "\n",
        "# Print the predicted class\n",
        "if predicted_class == 0:\n",
        "    print(\"Neutral\")\n",
        "elif predicted_class == 1:\n",
        "    print(\"Positive\")\n",
        "else:\n",
        "    print(\"Negative\")\n"
