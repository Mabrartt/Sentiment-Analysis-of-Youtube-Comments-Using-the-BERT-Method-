              "summary": "{\n  \"name\": \"df\",\n  \"rows\": 4992,\n  \"fields\": [\n    {\n      \"column\": \"text\",\n      \"properties\": {\n        \"dtype\": \"string\",\n        \"num_unique_values\": 4985,\n        \"samples\": [\n          \"I just ordered today, Ny first ipad.  I hope i can have it for 5 years\\u2764\\ud83d\\ude22\",\n          \"I like how you look down on the iPad because of the technology, but you get excited because you have a wallet for sale\\n\\ud83d\\ude02\",\n          \"Apples top end lineups are like formula 1 cars, just a push for better tech in supremely expensive way.\"\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"sentiment\",\n      \"properties\": {\n        \"dtype\": \"number\",\n        \"std\": 0,\n        \"min\": 0,\n        \"max\": 2,\n        \"num_unique_values\": 3,\n        \"samples\": [\n          0,\n          1,\n          2\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    }\n  ]\n}"
            }
